{
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "printWidth": 80,
  "endOfLine": "auto",
  "jsxSingleQuote": true,
  "bracketSpacing": true,
  "htmlWhitespaceSensitivity": "ignore",
  "overrides": [
    {
      "files": ["*.html", "*.scss"],
      "options": {
        "tabWidth": 4,
        "printWidth": 120
      }
    }
  ]
}
